.main {
    width: 100%;
    color: #222;
}

.header {
    display: flex;
    align-items: center;
    width: 100%;
    height: 300rpx;
    flex-direction: column;
}

.header image {
    width: 100rpx;
    height: 100rpx;
}

.title_header {
    margin-top: 150rpx;
    font-size: 50rpx;
    font-family: Microsoft Yahei;
    font-weight: 700;
    color: #0ab179;
}

.main .nav {
    height: 200rpx;
    display: flex;
    justify-content: space-between;
    flex: 1;
    margin-bottom: 40rpx;
}

.nav_item {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.main .nav .nav_item {
    margin: auto 64rpx;
}

.main .nav .logo {
    font-weight: bold;
}

.main .nav .nav_item .nav_item_text {
    font-size: 32rpx;
    margin-top: 20rpx;
}

.main .contant {
    height: 100%;
    width: 100%;
}

.main .contant .title {
    border-top: 20rpx solid #f2f2f2;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 32rpx;
}

.main .contant .title_text {
    padding-left: 26rpx;
}

.main .contant .contant_list {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: space-between;
    font-size: 32rpx;
}

.main .contant .contant_list .contant_name {
    padding: 26rpx;
}

.main .contant .contant_list .contant_people {
    padding: 26rpx 104rpx 26rpx 0;
}

.logo image {
    width: 80rpx;
    height: 80rpx;
}
